{
    "name": "tofiq/laravel-audit-trail",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tofiq\\AuditTrail\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tofiq Hamzai",
            "email": "tefo.ha27@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "9.x-dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Tofiq\\AuditTrail\\Tests\\": "tests/",
            "Tofiq\\AuditTrail\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tofiq\\AuditTrail\\AuditTrailServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
